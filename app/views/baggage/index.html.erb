<div id="container">
Fired up and ready to go!
<% if @bags.all.any? %>
  <% @bags.all.each do |bag| %>
    <h2><%= link_to bag.name, bag_path(bag) %></h2>
    <h2><%= link_to "Edit #{bag.name}", edit_bag_path(bag) %></h2>
    <h2><%= link_to "Delete #{bag.name}", bag_path(bag), method: :delete %></h2>
  <% end %>
<% else %>
  <h1>No bags yet ğŸ˜±</h1>
<% end %>

<a href="/baggage/new">Add a bag</a>
</div>
